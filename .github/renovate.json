{
  "extends": ["config:base"],
  "schedule": ["after 9pm and before 5am"],
  "prConcurrentLimit": 2,
  "dependencyDashboard": true,
  "packageRules": [
    {
      "matchPackageNames": ["zricethezav/gitleaks-action"],
      "enabled": false
    },
    {
      "matchUpdateTypes": ["patch", "pin", "digest"],
      "groupName": "patch",
      "prPriority": 50,
      "automerge": true
    },
    {
      "matchUpdateTypes": ["minor"],
      "groupName": "minor",
      "prPriority": 40,
      "automerge": true
    },
    {
      "matchUpdateTypes": ["major"],
      "groupName": "major",
      "prPriority": 20,
      "automerge": false
    },
    {
      "matchDepTypes": ["devDependencies"],
      "groupName": "devDependencies",
      "prPriority": 30,
      "automerge": true
    },
    {
      "matchCurrentVersion": "<1.0.0",
      "groupName": "beta",
      "prPriority": 10,
      "automerge": false
    }
  ]
}
